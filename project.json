{
  "$schema": "node_modules/nx/schemas/project-schema.json",
  "name": "cv",
  "prefix": "cv",
  "projectType": "application",
  "sourceRoot": "./src",
  "tags": [],
  "targets": {
    "build": {
      "configurations": {
        "production": {
          "buildOptimizer": false,
          "extractLicenses": false,
          "namedChunks": true,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production",
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "assets": ["./src/assets", "./src/favicon.ico", "./src/i18n"],
        "index": "./src/index.html",
        "inlineStyleLanguage": "scss",
        "main": "./src/main.ts",
        "outputPath": "docs",
        "polyfills": "zone.js",
        "scripts": ["node_modules/marked/marked.min.js", "node_modules/prismjs/prism.js"],
        "styles": [
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/primeng.min.css",
          "node_modules/primeng/resources/themes/saga-blue/theme.css",
          "node_modules/prismjs/themes/prism-okaidia.css",
          "./src/styles/styles.scss"
        ],
        "tsConfig": "./tsconfig.app.json"
      },
      "outputs": ["{options.outputPath}"]
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "cv:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["./src/**/*.html", "./src/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "serve": {
      "configurations": {
        "production": {
          "browserTarget": "cv:build:production"
        }
      },
      "defaultConfiguration": "production",
      "executor": "@angular-devkit/build-angular:dev-server"
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "jest.config.ts",
        "passWithNoTests": true
      },
      "outputs": ["{workspaceRoot}/coverage/{projectName}"]
    },
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/e2e/report"],
      "options": {
        "config": "./e2e/playwright.config.ts"
      }
    }
  }
}
